Imports System

Module Program
    Sub Main()

        Try
            Console.WriteLine("数字を入力してください")
            Dim num As Integer = Console.ReadLine()

            Console.WriteLine("あなたの選んだ数字は" & num & "です。")

        Catch ex As Exception

            Console.WriteLine("数字以外を入力しないでください")

        End Try


    End Sub
End Module
